[Unit]
Description=Verify successful boot after root swapped
Requires=multi-user.target
After=multi-user.target

[Service]
Type=oneshot
ExecCondition=/bin/sh -c "@BINDIR@/swap-root state | grep -q SWAPPING"
ExecStart=@BINDIR@/swap-root commit
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
